.integral-container .info {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 186rpx;
  background: #ffffff;
  border-top: 1rpx solid #cfcfcf;
}
.integral-container .left {
  margin-left: 28rpx;
  font-size: 24rpx;
}
.integral-container .left .glod {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 40rpx;
}
.integral-container .left .glod .glod-icon {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 60rpx;
  height: 60rpx;
  border-radius: 60rpx;
  background: #f5f5f5;
}
.integral-container .left .glod .glod-text {
  font-size: 34rpx;
  margin-left: 28rpx;
}
.integral-container .right {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: center;
  margin-right: 28rpx;
}
.integral-container .right .rules {
  font-size: 24rpx;
  margin-bottom: 40rpx;
}
.integral-container .select-time {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  margin-left: 28rpx;
}
.integral-container .select-time .time {
  font-size: 28rpx;
  margin-right: 10rpx;
}
.integral-container .select-time .modal {
  width: 100%;
  background: #fff;
}
.integral-container .list-group {
  padding-left: 28rpx;
  padding-right: 28rpx;
  background: #ffffff;
}
.iconfont {
  font-family: j-icon !important;
}
.list-desc {
  color: #999999 !important;
  font-size: 22rpx !important;
}
.earning {
  color: #fc5366;
}
.expense {
  color: #333;
}

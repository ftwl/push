.vip-container {
  width: 100%;
}
.vip-container .card {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #ffffff;
  height: 535rpx;
  border-top: 1rpx solid #dedede;
}
.vip-container .card .card-image {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: space-between;
  width: 694rpx;
  height: 354rpx;
  background-image: url('https://tuiyixia.oss-cn-shanghai.aliyuncs.com/image/vip-card.png?OSSAccessKeyId=LTAINo4457UW83Sw&Expires=1575255975&Signature=hKm7CwoISgEniWH6iElwQkXVyG0%3D');
  background-size: 100%;
  margin-top: 24rpx;
  margin-bottom: 30rpx;
}
.vip-container .card .card-image .card-info {
  margin-left: 28rpx;
}
.vip-container .card .card-image .card-info .title {
  width: 167rpx;
  height: 46rpx;
  padding-top: 50rpx;
}
.vip-container .card .card-image .card-info .title image {
  width: 100%;
  height: 100%;
}
.vip-container .card .card-image .vip {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
  margin-top: 100rpx;
}
.vip-container .card .card-image .vip .user-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-left: 24rpx;
}
.vip-container .card .card-image .vip .user-info .nickname {
  color: #f5cf72;
  font-size: 34rpx;
}
.vip-container .card .card-image .vip .user-info text {
  color: #8d8f93;
  font-size: 22rpx;
}
.vip-container .card .card-image .price {
  margin-top: 47rpx;
  margin-right: 28rpx;
}
.vip-container .introduce {
  background: #ffffff;
  margin-top: 20rpx;
  padding: 18rpx 28rpx;
}
.vip-container .introduce .introduce-title {
  font-size: 28rpx;
  font-weight: bold;
}
.cell {
  color: #f5cf72;
  font-size: 30rpx;
}
.pay-button {
  background: #f2ca69 !important;
}
.iconfont {
  font-family: j-icon !important;
  color: #f2ca69 !important;
}
.left-section {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
}
.left-section .desc {
  font-size: 22rpx;
  color: #919699;
}
